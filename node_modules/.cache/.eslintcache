,"/home/cedcoss/Anshika/todo_firebase_redux/src/index.js",[],[],"/home/cedcoss/Anshika/todo_firebase_redux/src/reportWebVitals.js",[],[],"/home/cedcoss/Anshika/todo_firebase_redux/src/App.js",["74","75","76"],[],"import { useEffect, useRef, useState } from 'react';\nimport './App.css';\nimport TodoForm from './components/TodoForm';\nimport firebaseApp from './Firebase';\nimport {getDatabase, onValue, ref } from 'firebase/database'\nimport { useDispatch } from 'react-redux';\nimport { addText, checkHandle, deleteAllData, deleteCompData, deleteData, editText } from './features/todoSlice';\nfunction App() {\n\n  const db = getDatabase(firebaseApp)\n  var dispatch = useDispatch()\n  var [btnTxt,setBtnTxt]=useState('Add New Task')\n  var [title,setTitle]=useState('')\n  var [todolist,setTodoList]=useState([])\n  var inpRef = useRef()\n  const todoRef = ref(db,\"/todos\");\n\n  useEffect(()=>{\n    onValue(todoRef, (snapshot)=>{\n      const todos = snapshot.val()\n      const newTodoList=[]\n      for(let id in todos){\n        newTodoList.push({id,...todos[id]});\n      }\n      setTodoList(newTodoList)\n    });\n  },[db])\n  // Input Handler Function\n  const inpChangeHandler=(e)=>{\n    setTitle(e.target.value)\n  }\n  // function adds data in a list\n  const addData=()=>{\n    if(inpRef.current.value!=''){\n      if(btnTxt=='Add New Task'){\n        const todo={\n          title,\n          done:false\n        }\n        dispatch(addText({todo:todo,btnTxt:btnTxt}))\n      }\n      else{\n        dispatch(addText(inpRef.current.value))\n        setBtnTxt('Add New Task')\n      }\n      inpRef.current.value=''\n    }\n    else{\n      alert('Fill Details')\n    }\n  }\n  // Function Edits Data\n  const editHandler=(id,title)=>{\n    inpRef.current.value=title\n    dispatch(editText(id))\n    setBtnTxt('Update Text')\n  }\n  // function check Uncheck the completed and incompleted tasks \n  const checkHandler=(id,item)=>{\n    dispatch(checkHandle(item))\n  }\n  // Function deletes Task\n  const deleteHandler=(id)=>{\n    dispatch(deleteData(id))\n  }\n  // Function Deletes All Tasks\n  const deleteAll =()=>{\n    dispatch(deleteAllData())\n  }\n  // Function deletes Completed tasks\n  const deleteCompleted =()=>{\n    dispatch(deleteCompData(todolist))\n  }\n\n  return (\n    <div className=\"container\">\n      <h1>TodoInput</h1>\n      <TodoForm btnTxt={btnTxt} addData={addData} inpChangeHandler={inpChangeHandler} inpRef={inpRef} todolist={todolist} editHandler={editHandler} deleteAll={deleteAll} deleteHandler={deleteHandler} checkHandler={checkHandler} deleteCompleted={deleteCompleted}/>\n    </div>\n  );\n}\n\nexport default App;",[],"/home/cedcoss/Anshika/todo_firebase_redux/src/features/store.js",[],[],"/home/cedcoss/Anshika/todo_firebase_redux/src/Firebase.js",[],[],"/home/cedcoss/Anshika/todo_firebase_redux/src/features/todoSlice.js",["77"],[],"/home/cedcoss/Anshika/todo_firebase_redux/src/components/TodoForm.js",["78"],[],{"ruleId":"79","severity":1,"message":"80","line":7,"column":10,"nodeType":"81","messageId":"82","endLine":7,"endColumn":17},{"ruleId":"79","severity":1,"message":"83","line":7,"column":19,"nodeType":"81","messageId":"82","endLine":7,"endColumn":27},{"ruleId":"79","severity":1,"message":"84","line":11,"column":7,"nodeType":"81","messageId":"82","endLine":11,"endColumn":12},{"ruleId":"79","severity":1,"message":"85","line":12,"column":7,"nodeType":"81","messageId":"82","endLine":12,"endColumn":15},{"ruleId":"86","severity":1,"message":"87","line":29,"column":5,"nodeType":"88","endLine":29,"endColumn":9,"suggestions":"89"},{"ruleId":"90","severity":1,"message":"91","line":37,"column":28,"nodeType":"92","messageId":"93","endLine":37,"endColumn":30},{"ruleId":"90","severity":1,"message":"94","line":38,"column":14,"nodeType":"92","messageId":"93","endLine":38,"endColumn":16},{"ruleId":"95","severity":1,"message":"96","line":87,"column":24,"nodeType":"97","messageId":"98","endLine":87,"endColumn":26},{"ruleId"